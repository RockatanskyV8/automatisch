## Options section
source "$HOME/.scripts/zsh_config/options"
## Keybindings section
source "$HOME/.scripts/zsh_config/keys"
## Shortcuts
[ -f "$HOME/.config/shortcutrc" ] && source "$HOME/.config/shortcutrc"
## Alias
[ -f "$HOME/.scripts/zsh_config/aliases" ] && source "$HOME/.scripts/zsh_config/aliases"
## Theming section  
source "$HOME/.scripts/zsh_config/theming"
## Prompt
source "$HOME/.scripts/zsh_config/prompt"
## Git
source "$HOME/.scripts/zsh_config/git_config"
## Color man pages
source "$HOME/.scripts/zsh_config/manpage_color"
## Plugin Section
source "$HOME/.scripts/zsh_config/plugins"

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
# Lines configured by zsh-newuser-install
HISTFILE=~/.cache/.histfile
HISTSIZE=1000
SAVEHIST=1000
setopt autocd beep
bindkey -e
# End of lines configured by zsh-newuser-install
# The following lines were added by compinstall
zstyle :compinstall filename '/home/lucas/.zshrc'

#autoload -Uz compinit
#compinit
# End of lines added by compinstall

